@{
    ViewBag.Title = "Insert Warhead";
}

@if (ViewBag.insertStatus != null) {
    bool insertStatus = ViewBag.insertStatus;
    if (insertStatus) { 
        <h4>Başarıyla Eklendi</h4>
    }else{
        <h4>Sorun Çıktı</h4>
    }
}

<h2>Insert Warhead</h2>
<form action="#" method="post">
    <h3>Başlık Adı:</h3>
    <input type="text" name="WarheadName" />
    <input type="submit" value="Gönder"/>
</form>
