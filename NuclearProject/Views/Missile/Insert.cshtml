@model List<NuclearProject.Models.WarheadType>
<h2>Add</h2>
<form action="#" method="post">
    <h3>Başlık Tipi</h3>
    <select name="WarheadTypeId">
        @foreach (NuclearProject.Models.WarheadType wt in Model) { 
            <option value="@wt.WarheadTypeId">@wt.WarheadTypeName</option>
        }
    </select>
    <h3>Füze Adı</h3>
    <input type="text" name="MissileName" />
    <h3>Menzili</h3>
    <input type="text" name="MissileRange" />
    <h3>YakıtTipi</h3>
    <input type="text" name="FuelType" />
    <br />
    <input type="submit" value="Gönder" />

</form>